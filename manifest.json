{
  "name": "DW Inbox Opener",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Opens X number of new tabs from the Dreamwidth inbox",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.dreamwidth.org/inbox/?page=*.&view=unread"],
      "js": ["openParentLink.js"]
    }
  ]
}